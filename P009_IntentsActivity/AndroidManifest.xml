<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lquispe.android.p009_intentsactivity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <!-- ruta fisica -->
        <activity
            android:name="com.lquispe.android.p009_intentsactivity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.lquispe.android.p009_intentsactivity.Actividad2"
            android:label="@string/app_name" >
            <intent-filter>
                <!-- ruta logica -->
                <action android:name="com.lquispe.android.p009_intentsactivity.Actividad2" />
				<!-- Default cuando la llama otra actividad, Launcher es la que carga al inicio -->
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
